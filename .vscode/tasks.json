{
    "version": "2.0.0",
    "command": "bash",
    "options": {
        "cwd": "${workspaceRoot}"
    },
    "args": [
        "-c"
    ],
    "tasks": [
        {
            "label": "build",
            "args": [
                "yarn build && yarn copy"
            ]
        },
        {
            "label": "clean",
            "args": [
                "yarn clean"
            ]
        }
    ]
}
